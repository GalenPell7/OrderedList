public class Node<T>
{
    private T data;
    private Node link;

    public Node(T d, Node l)
    {
        data=d;
        link=l;
    }
    public T getData()
    {
        return data;
    } //accessors
    public Node getLink()
    {
        return link;
    }
    public void setData(T d)
    {
        data=d;
    } //mutators that modify data of the node
    public void setLink(Node l)
    {
        link=l;
    }// another mutator
}
